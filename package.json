{
  "name": "sql-statement",
  "description": "Tiny SQL Statement Builder",
  "version": "1.1.0",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/sql-statement/issues"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/sqlite3": "^5.1.0",
    "jsdoc": "^4.0.4",
    "jshint": "^2.13.6",
    "mocha": "^11.7.2",
    "mysql2": "^3.15.1",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./sql-statement.d.ts",
        "default": "./esm/sql.mjs"
      },
      "require": "./sql.js"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "esm/*.mjs",
    "*.js",
    "*.d.ts"
  ],
  "homepage": "https://github.com/kawanet/sql-statement",
  "jshintConfig": {
    "esversion": 6,
    "mocha": true,
    "node": true,
    "undef": true,
    "unused": false
  },
  "keywords": [
    "mysql",
    "pg",
    "placeholder",
    "postgresql",
    "sql",
    "sqlite"
  ],
  "license": "MIT",
  "main": "sql.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kawanet/sql-statement.git"
  },
  "scripts": {
    "build": "make",
    "fixpck": "fixpck",
    "prepack": "npm run build && npm test",
    "test": "make test"
  },
  "typings": "./sql-statement.d.ts"
}
